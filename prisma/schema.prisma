datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Usuario {
  id       String   @id @default(uuid())
  nome     String
  senha    String
  criadoEm DateTime @default(now())
}

model Formulario {
  id        String   @id @default(uuid())
  nome      String
  sobrenome String
  email     String
  telefone  String
  nomePet   String
  especie   String
  raca      String
  criadoEm  DateTime @default(now())
}
